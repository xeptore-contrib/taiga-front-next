{
  "build": {
    "dockerfile": "./Dockerfile",
    "context": ".",
    "options": [
      "--pull",
      "--progress=plain"
    ],
    "args": {
      "USER_UID": "1000",
      "USER_GID": "1000",
      "GIT_VERSION": "2.52.0",
      "DUST_VERSION": "v1.2.3",
      "BAT_VERSION": "v0.26.1",
      "YQ_VERSION": "v4.50.1",
      "JQ_VERSION": "1.8.1",
      "NODE_VERSION": "v12.22.12"
    }
  },
  "runArgs": [
    "--hostname=dev-taiga-front-next",
    "--cap-drop=ALL"
  ],
  "capAdd": [
    "CHOWN"
  ],
  "init": true,
  "shutdownAction": "stopContainer",
  "name": "taiga-front-next-dev",
  "updateRemoteUserUID": true,
  "features": {},
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.suggest.enabled": true
      },
      "extensions": [
        "ms-azuretools.vscode-containers",
        "ms-azuretools.vscode-docker",
        "jeremyrajan.webpack",
        "amodio.tsl-problem-matcher",
        "streetsidesoftware.code-spell-checker",
        "streetsidesoftware.code-spell-checker-persian"
      ]
    }
  },
  "postCreateCommand": {},
  "workspaceMount": "type=bind,source=${localWorkspaceFolder},destination=/workspaces/taiga-front-next",
  "workspaceFolder": "/workspaces/taiga-front-next",
  "forwardPorts": [],
  "portsAttributes": {},
  "privileged": false,
  "containerUser": "vscode",
  "remoteUser": "vscode"
}
