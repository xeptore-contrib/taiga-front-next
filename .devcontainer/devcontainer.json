{
  "build": {
    "dockerfile": "./Dockerfile",
    "context": ".",
    "options": [
      "--pull",
      "--progress=plain"
    ],
    "args": {
      "USER_UID": "1000",
      "USER_GID": "1000",
      "GIT_VERSION": "v2.53.0",
      "GIT_SHA512": "7b8bb8b79e2225d168ce1e47466f1df7646b71f22776f1167c4680ec98ca438e99e8e081227cf4108440e7c5ac79f1170b92029dfd0c59ffc6da72dd7b30e9eb",
      "DUST_VERSION": "v1.2.4",
      "DUST_SHA256_AMD64": "707cfdbfb9d2dc536f8c3853815bbe98a01012f2772463835edae06816551160",
      "DUST_SHA256_ARM64": "1903296e662a80a504b132525c9360b503de3b900970e1245268605fc65e366d",
      "BAT_VERSION": "v0.26.1",
      "BAT_SHA256_AMD64": "726f04c8f576a7fd18b7634f1bbf2f915c43494c1c0f013baa3287edb0d5a2a3",
      "BAT_SHA256_ARM64": "422eb73e11c854fddd99f5ca8461c2f1d6e6dce0a2a8c3d5daade5ffcb6564aa",
      "YQ_VERSION": "v4.52.2",
      "YQ_SHA_AMD64": "a74bd266990339e0c48a2103534aef692abf99f19390d12c2b0ce6830385c459",
      "YQ_SHA_ARM64": "c82856ac30da522f50dcdd4f53065487b5a2927e9b87ff637956900986f1f7c2",
      "JQ_VERSION": "1.8.1",
      "JQ_SHA256_AMD64": "020468de7539ce70ef1bceaf7cde2e8c4f2ca6c3afb84642aabc5c97d9fc2a0d",
      "JQ_SHA256_ARM64": "6bc62f25981328edd3cfcfe6fe51b073f2d7e7710d7ef7fcdac28d4e384fc3d4",
      "EZA_VERSION": "v0.23.4",
      "EZA_SHA256_AMD64": "0a1c72faefcb511b08b1b8470aedf4a6a3a94c4d56f8f65da3cb7d8c5de051f1",
      "EZA_SHA256_ARM64": "366e8430225f9955c3dc659b452150c169894833ccfef455e01765e265a3edda",
      "SHELLCHECK_VERSION": "v0.11.0",
      "SHELLCHECK_SHA256_AMD64": "b7af85e41cc99489dcc21d66c6d5f3685138f06d34651e6d34b42ec6d54fe6f6",
      "SHELLCHECK_SHA256_ARM64": "68a8133197a50beb8803f8d42f9908d1af1c5540d4bb05fdfca8c1fa47decefc",
      "NODE_VERSION": "v12.22.12",
      "OMZ_COMMIT": "e0766905519fbc7982a590a195ee7c3e8bb594e8",
      "ZSH_AUTOSUGGESTIONS_COMMIT": "85919cd1ffa7d2d5412f6d3fe437ebdbeeec4fc5"
    }
  },
  "runArgs": [
    "--name=taiga-front-next-dev",
    "--hostname=taiga-front-next-dev",
    "--cap-drop=ALL"
  ],
  "capAdd": [
    "CHOWN"
  ],
  "init": true,
  "shutdownAction": "stopContainer",
  "name": "taiga-front-next-dev",
  "updateRemoteUserUID": true,
  "features": {},
  "customizations": {
    "vscode": {
      "settings": {
        "cSpell.enabled": true,
        "git.confirmSync": false,
        "git.enableSmartCommit": false,
        "git.autorefresh": true,
        "git.openRepositoryInParentFolders": "never",
        "search.followSymlinks": false,
        "search.useIgnoreFiles": true,
        "search.exclude": {
          "**/node_modules": true,
          "**/dist": true,
          "**/build": true,
          "**/.git": true
        },
        "files.watcherExclude": {
          "**/node_modules/**": true,
          "**/dist/**": true,
          "**/build/**": true
        },
        "eslint.run": "onType",
        "problems.showCurrentInStatus": true,
        "terminal.integrated.suggest.enabled": false,
        "terminal.integrated.copyOnSelection": true,
        "terminal.integrated.defaultProfile.linux": "zsh",
        "terminal.integrated.scrollback": 20000,
        "editor.formatOnSave": true,
        "editor.formatOnPaste": false,
        "editor.formatOnType": false,
        "editor.defaultFormatter": null,
        "scm.defaultViewMode": "tree",
        "files.trimTrailingWhitespace": true,
        "files.insertFinalNewline": true,
        "files.eol": "\n",
        "files.autoSave": "off",
        "files.encoding": "utf8",
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.detectIndentation": false,
        "editor.renderWhitespace": "boundary",
        "editor.renderControlCharacters": false,
        "editor.minimap.enabled": false,
        "git.detectSubmodules": false,
        "git.autofetch": false,
        "typescript.tsdk": "node_modules/typescript/lib",
        "typescript.enablePromptUseWorkspaceTsdk": true
      },
      "extensions": [
        "anthropic.claude-code",
        "amodio.tsl-problem-matcher",
        "jeremyrajan.webpack",
        "ms-azuretools.vscode-containers",
        "ms-azuretools.vscode-docker",
        "Orta.vscode-jest",
        "streetsidesoftware.code-spell-checker-persian",
        "streetsidesoftware.code-spell-checker"
      ]
    }
  },
  "postCreateCommand": {},
  "workspaceMount": "type=bind,source=${localWorkspaceFolder},destination=/workspaces/taiga-front-next",
  "workspaceFolder": "/workspaces/taiga-front-next",
  "forwardPorts": [],
  "portsAttributes": {
    "4200": {
      "label": "Dev Server",
      "onAutoForward": "openBrowser",
      "elevateIfNeeded": false,
      "protocol": "http",
      "requireLocalPort": true
    }
  },
  "privileged": false,
  "containerUser": "vscode",
  "remoteUser": "vscode"
}
